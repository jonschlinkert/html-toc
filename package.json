{
  "name": "html-toc",
  "description": "Generate a HTML table of contents using cheerio.",
  "version": "1.0.0",
  "homepage": "https://github.com/jonschlinkert/html-toc",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/html-toc",
  "bugs": {
    "url": "https://github.com/jonschlinkert/html-toc/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "mocha"
  },
  "keywords": [
    "html",
    "table of contents",
    "toc"
  ],
  "dependencies": {
    "cheerio": "^0.22.0",
    "extend-shallow": "^2.0.1",
    "markdown-slug": "^0.1.1"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.11",
    "js-beautify": "^1.6.11",
    "mocha": "^3.2.0"
  },
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "remarkable",
        "markdown-toc",
        "breakdance"
      ]
    },
    "lint": {
      "reflinks": true
    }
  }
}
